{
  "name": "@workspace/next",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/next.cjs",
      "import": "./dist/next.js"
    }
  },
  "main": "./dist/next.cjs",
  "module": "./dist/next.js",
  "types": "./dist/main.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "dev": "vite",
    "lint:fix": "pnpm run lint --fix",
    "lint": "eslint --cache --max-warnings 0 --report-unused-disable-directives .",
    "preview": "vite preview",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "firebase": "^9.23.0"
  },
  "dependencies": {
    "@vueuse/firebase": "^10.2.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.44.0",
    "jsdom": "^22.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.12",
    "vite-plugin-dts": "^3.1.0",
    "vite-plugin-vue-devtools": "^0.4.14",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.4"
  }
}
